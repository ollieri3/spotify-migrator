{
  "name": "spotify-migrator",
  "private": true,
  "version": "0.0.1",
  "description": "A simple app to help you migrate to a new Spotify account",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "prod": "stencil build --prod",
    "serve:prod": "stencil build --prod && http-server ./www",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "^0.10.0",
    "@stencil/router": "^0.2.2"
  },
  "devDependencies": {
    "@stencil/sass": "0.0.5",
    "@types/jest": "^21.1.1",
    "http-server": "^0.11.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-app-starter.git"
  },
  "author": "Oliver Ridsdale",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
